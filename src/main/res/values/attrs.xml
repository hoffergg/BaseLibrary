<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InfoItemView">
        <attr name="itemIcon" format="reference" />
        <attr name="itemTitle" format="reference|string" />
        <attr name="itemNextIcon" format="reference" />
    </declare-styleable>
    <declare-styleable name="SettingItemView">
        <attr name="settingIcon" format="reference" />
        <attr name="settingTitle" format="reference|string" />
        <attr name="settingText" format="reference|string" />
        <attr name="settingHint" format="reference|string" />
        <attr name="settingImage" format="reference" />
        <attr name="settingContentLeftDrawable" format="reference" />
        <attr name="settingNextIcon" format="reference" />
        <attr name="settingLayout" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>
</resources>